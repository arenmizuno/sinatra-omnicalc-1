<h1>
  Generate random number
</h1>
<form action="/random/results">
  <div>
    <label for="minimum">
      Minimum
    </label>
    <input id="minimum" type="text" name="users_minimum" placeholder="E.g. 1.5">
  </div>

  <div>
    <label for="maximum">
      Maximum
    </label>
    <input id="maximum" type="text" name="users_maximum" placeholder="E.g. 4.5">
  </div>

  <button> 
  Pick random number
  </button>
</form>
